  "license": "MPL-2.0",
  "dependencies": {
    "gettext-parser": "^4.0.2",
    "htmlparser2": "^4.0.0"
  },
  "devDependencies": {
    "eslint-config-fxa": "^3.0.0",
    "grunt": "^1.0.4",
